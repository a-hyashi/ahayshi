{
  "name": "031",
  "version": "1.0.0",
  "description": "CMS-Buddy's theme",
  "main": "index.js",
  "config": {
    "htmlTemplateDir": "../../ACRE-theme/acre/theme_materials/031/html_templates/",
    "projectDevDir": "./devStuff/",
    "commonDevDir": "../common/devStuff/",
    "datajsonDir": "./styleguide_assets/datajson/",
    "htmlDir": "./styleguide_assets/html/",
    "specmdDir": "./unittest/",
    "aigisymlDir": "./devStuff/"
  },
  "bin": {
    "btool": "file:./bin/"
  },
  "scripts": {
    "start": "gulp",
    "test": "gulp sasslint",
    "dev": "gulp run-full",
    "dev-compact": "gulp run-compact",
    "pc": "gulp --pc",
    "sp": "gulp --sp",
    "all": "gulp --all",
    "build": "gulp build --build",
    "btool-mdaf": "rm -rf ${npm_package_config_datajsonDir} && gulp make-allparts-datajson",
    "btool-mh": "rm -rf ${npm_package_config_htmlDir} && gulp make-html",
    "btool-aigis": "rm -rf ${npm_package_config_specmdDir} && gulp make-aigis",
    "btool-unittest": "npm run btool-mdaf && npm run btool-mh && npm run btool-aigis",
    "aigis": "gulp aigis",
    "init": "gulp init",
    "update-css": "gulp sass && gulp copy-css",
    "update-parts": "npm run btool-unittest && npm run aigis"
  },
  "author": "Shintaro Suzuki",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^4.0.7",
    "@types/handlebars": "^4.0.36",
    "@types/lodash": "^4.14.93",
    "browser-sync": "2.18.13",
    "commander": "^2.13.0",
    "del": "2.2.2",
    "event-stream": "3.3.4",
    "fs": "0.0.1-security",
    "fs-extra": "^4.0.3",
    "gulp": "3.9.1",
    "gulp-aigis": "1.3.0",
    "gulp-autoprefixer": "3.1.1",
    "gulp-csslint": "1.0.0",
    "gulp-debug": "3.1.0",
    "gulp-if": "2.0.2",
    "gulp-imagemin": "3.1.1",
    "gulp-notify": "3.0.0",
    "gulp-plumber": "1.1.0",
    "gulp-rename": "1.2.2",
    "gulp-sass": "3.2.0",
    "gulp-sass-lint": "1.3.4",
    "gulp-size": "2.1.0",
    "gulp-sourcemaps": "2.6.1",
    "gulp-webserver": "0.9.1",
    "handlebars": "^4.0.11",
    "js-beautify": "^1.7.5",
    "js-yaml": "^3.10.0",
    "json-schema-parser": "^0.1.0",
    "json2md": "^1.6.1",
    "lodash": "^4.17.4",
    "minimist": "1.2.0",
    "moment": "^2.20.1",
    "node-aigis": "1.4.1",
    "node-sass": "4.8.1",
    "run-sequence": "1.2.2",
    "sassdoc": "2.4.0",
    "tv4": "^1.3.0"
  },
  "dependencies": {
    "jsprim": "^2.0.0"
  }
}
